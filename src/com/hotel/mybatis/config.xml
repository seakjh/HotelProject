		<mapper resource="com/hotel/mybatis/PayMentMapper.xml"/>
		<mapper resource="com/hotel/mybatis/ReservationMapper.xml"/>
		<mapper resource="com/hotel/mybatis/ReserveTableMapper.xml"/>
		<mapper resource="com/hotel/mybatis/RoomMapper.xml"/>
